{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": 0,
      "eol-last": 0,
      "import/no-extraneous-dependencies": 0,
      "no-console": 0,
      "no-underscore-dangle": 0,
      "no-restricted-globals": 0,
      "linebreak-style": 0,
      "operator-linebreak": 0,
      "no-alert": 0,
      "consistent-return": 0,
      "no-prototype-builtins": 0,
      "no-return-assign": 0,
      "class-methods-use-this": 0,
      "quotes": ["error", "single"],
      "object-shorthand": ["error", "always"],
      "no-undef": "off",
      "import/first": "off",
      "import/extensions": "off",
      "no-multiple-empty-lines": "off",
      "no-trailing-spaces": "off",
      "arrow-body-style": "off",
      "arrow-parens": "off",
      "no-use-before-define": "off",
      "no-plusplus": "off",
      "comma-dangle": "off"
    }
}
